
fn lpf(){    
    self  = 0
    println(100101010)
    fn perform(input:float,fb:float){
        res = input*(1-fb) + self*fb
        self = res
        return res
    }
    return perform
}
mylpf = lpf()
global = 2
fn dsp(time:float)->float{
    fb = (sin(time/1000+sin(time/40050)*15)+1)/2.01
    return mylpf((random()/global),fb)
}